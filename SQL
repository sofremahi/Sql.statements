drop table TB_NAME;


CREATE TABLE TB_NAME0
(
    ID  NUMBER(12) PRIMARY KEY,
    TITLE VARCHAR2(40) NOT NULL
);

CREATE TABLE TB_NAME1
(
    ID          NUMBER(12) PRIMARY KEY,
    TITLE         VARCHAR2(40) NOT NULL,
    NAME0_ID NUMBER(12)
);

ALTER TABLE TB_NAME1
    ADD CONSTRAINT fk_name
        FOREIGN KEY (NAME0_ID)
            REFERENCES TB_NAME0 (ID);


CREATE TABLE TB_NAME
(
    ID                  NUMBER(12) PRIMARY KEY,
    DATE             DATE,
    PRICE NUMERIC(18,2)
);

commit;
rollback;

INSERT INTO TB_NAME0 (ID , TITLE)
VALUES ( 1 , 'hello world' );
